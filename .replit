run = "cd draftshift-web && python run_server.py"
entrypoint = "draftshift-web/run_server.py"

[env]
PYTHONPATH = "/home/runner/saoriverse-console/draftshift-web:/home/runner/saoriverse-console"

[packager]
language = "python3"
ignoreDefaultPythonPackages = true

[packager.features]
packageSearch = true
guessImports = true
enableGuessImports = true

[nix]
channel = "stable-23_05"
