{
  "deno.enable": true,
  "deno.lint": false,
  "deno.unstable": true,
  "typescript.preferences.strict": false,
  "typescript.suggest.autoImports": false,
  "python.defaultInterpreterPath": "./venv/Scripts/python.exe",
  "files.associations": {
    "*.ts": "typescript"
